m: prevState.team.filter(petItem => petItem !== pet)\n    }))\n  }\n\n  setHoveredPet = (pet) => {\n    this.setState({hoveredPet: pet})\n  }\n\n  startBattle = () => {\n\n    console.log(this.state.team)\n\n    // createTeam()\n\n    // create team pets with hp, dmg, abilities\n    // create game\n    // create boss\n\n  }\n\n  // createTeam = () => {\n\n  //   fetch(`${BASEURL}/newgame`, configPetObj() )\n  //   .then(resp => resp.json())\n  //   .then(json => {\n  //     console.log\n  //   })\n\n\n\n  //}\n\n  configPetObj = () => {\n    return {\n      method: 'POST',\n      headers: {\n        'Content-Type': 'application/json',\n        'Accept': 'application/json'\n      },\n      body: JSON.stringify({\n        team: this.state.team\n      })\n    }\n  }\n\n\n  render() {\n    const {apiPets, team, hoveredPet, footerInfo, page} = this.state\n    return (\n      <div className=\"App\">\n        \n        <header className=\"\">\n          <Header/>\n        </header>\n  \n        <body className=\"\">\n        <BrowserRouter>\n          <Main addPet={this.addPet} \n              removePet={this.removePet} \n              team={team} \n              pets={apiPets} \n              hoveredPet={hoveredPet} //state\n              setHoveredPet={this.setHoveredPet}\n              />\n        </BrowserRouter>\n        </body>\n  \n        <footer className=\"\">\n          <Footer info={footerInfo} page={page} handleClick={this.startBattle}/>\n        </footer>\n        \n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/michaellee/Flatiron/mod_4/Project_4/front-PetBattle/src/components/Footer.js",["19","20"],{"ruleId":"21","severity":1,"message":"22","line":4,"column":8,"nodeType":"23","messageId":"24","endLine":4,"endColumn":25},{"ruleId":"21","severity":1,"message":"25","line":6,"column":8,"nodeType":"23","messageId":"24","endLine":6,"endColumn":23},{"ruleId":"21","severity":1,"message":"26","line":7,"column":7,"nodeType":"23","messageId":"24","endL